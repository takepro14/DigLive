[
  {
    "name": "diglive-front",
    "image": "454636438913.dkr.ecr.ap-northeast-1.amazonaws.com/diglive-front:latest",
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "front",
        "awslogs-group": "/ecs/front"
      }
    },
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 80
      }
    ],
    "command": [
      "yarn", "start"
    ],
    "environment": [
      {
        "name": "NODE_ENV",
        "value": "production"
      },
      {
        "name": "WORKDIR",
        "value": "app"
      },
      {
        "name": "HOST",
        "value": "0.0.0.0"
      },
      {
        "name": "PORT",
        "value": "80"
      }
    ],
    "secrets": [
      {
        "name": "S3_BUCKET",
        "valueFrom": "S3_BUCKET"
      }
    ]
  }
]