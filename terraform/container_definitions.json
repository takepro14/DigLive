[
  {
    "name": "dig-live",
    "image": "454636438913.dkr.ecr.ap-northeast-1.amazonaws.com/ecr-dig-live:latest",
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "nginx",
        "awslogs-group": "/ecs/dig-live"
      }
    },
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 3000
      }
    ]
  }
]